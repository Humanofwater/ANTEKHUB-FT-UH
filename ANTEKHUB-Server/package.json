{
  "name": "antekhub-server",
  "version": "1.0.0",
  "description": "Server backend untuk ANTEKHUB (Express + Sequelize + PostgreSQL)",
  "main": "src/server.js",
  "type": "commonjs",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon src/server.js",
    "start": "node src/server.js",
    "db:create": "npx sequelize-cli db:create",
    "db:drop": "npx sequelize-cli db:drop",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "db:seed": "node scripts/run-seed.js",
    "db:seed:all": "npx sequelize-cli db:seed:all",
    "db:seed:undo": "node scripts/run-seed-undo.js",
    "db:seed:undo:all": "npx sequelize-cli db:seed:undo:all",
    "db:reset": "npm run db:drop && npm run db:create && npm run db:migrate && npm run db:seed",
    "db:test": "node scripts/test-db.js",
    "api:test": "node scripts/run-test.js",
    "api:test:all": "node scripts/run-test-all.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "cors": "^2.8.5",
    "dayjs": "^1.11.19",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "firebase-admin": "^13.6.0",
    "googleapis": "^164.1.0",
    "helmet": "^8.1.0",
    "joi": "^18.0.1",
    "jsonwebtoken": "^9.0.2",
    "midtrans": "^1.0.7",
    "midtrans-client": "^1.4.3",
    "mkdirp": "^3.0.1",
    "morgan": "^1.10.1",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "postmark": "^4.0.5",
    "sequelize": "^6.37.7",
    "streamifier": "^0.1.1",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "nodemon": "^3.1.10",
    "sequelize-cli": "^6.6.3"
  }
}
